{
  "manifest_version": 3,
  "name": "DNS Lookup Tool",
  "version": "1.0",
  "description": "Resolve DNS records for one or multiple hostnames.",
  "permissions": ["dns", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
